package exercise_3;

public class FuncList implements Func1 {
	
	public static boolean isOdd(int x) {
		
		return (x % 2 == 1);
	
	}
	
	public String passCheck(int point, String name) {
		
		return name + "さんは" + (point > 65 ? "合格" : "不合格");
		
	}
	
	public void process(int x, boolean odd) {
		
	}

}
